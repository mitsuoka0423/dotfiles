[user]
	name = Takahiro Mitsuoka
	email = tmitsuoka0423@gmail.com
[core]
	quotepath = false
	autocrlf = false
	safecrlf = true
	ignorecase = false
	excludesfile = /Users/mitsu/.gitignore_global
[http]
	postBuffer = 200M
[credential]
	helper = wincred
[push]
	default = simple
[alias]
	alias = !git config --get-regexp '^alias\\.' | sed 's/alias\\.\\([^ ]*\\) \\(.*\\)/\\1\\\t => \\2/' | sort
	c = commit -m
	ch = checkout
	drag = pull --rebase
	st = status
	sync = git pull --rebase && git push
[credential "https://source.developers.google.com"]
	helper = gcloud.sh
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	ff = only
