[user]
	name = Takahiro Mitsuoka
	email = mono0423@gmail.com
[core]
	quotepath = false
	autocrlf = false
	safecrlf = true
	ignorecase = false
	excludesfile = /Users/mitsu/.gitignore_global
[http]
	postBuffer = 200M
[credential]
	helper = wincred
[push]
	default = simple
[alias]
	st = status
	ch = checkout
	c = commit -m
[credential "https://source.developers.google.com"]
	helper = gcloud.sh
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
